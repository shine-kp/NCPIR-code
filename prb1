def reverse_and_add(n, k):
    def is_palindrome(x):
        s = str(x)
        return s == s[::-1]

    for i in range(1, k + 1):
        rev_n = int(str(n)[::-1])
        n = n + rev_n
        if is_palindrome(n):
            return [i, n]
    return [-1, -1]

print(reverse_and_add(89, 30))  
print(reverse_and_add(56, 10)) 
